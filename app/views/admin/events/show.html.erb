<div class="page-header">
  <%= link_to events_path, class: 'btn btn-default' do %>
    All Events
  <% end %>
  <%= link_to edit_event_path(@event), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show event</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @event.name %></dd>
  <dt>Start time:</dt>
  <dd><%= @event.start_time %></dd>
  <dt>Recurring:</dt>
  <dd><%= @event.recurring %></dd>
  <dt>Role:</dt>
  <dd><%= @event.role %></dd>
  <dt>End time:</dt>
  <dd><%= @event.end_time %></dd>
  <dt>Type:</dt>
  <dd><%= @event.type %></dd>
</dl>
