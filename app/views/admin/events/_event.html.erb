<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= event.start_time %>
  </p>

  <p>
    <strong>Recurring:</strong>
    <%= event.recurring %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= event.role %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= event.end_time %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= event.type %>
  </p>

</div>
